{
  "name": "enbrands-interaction-backed",
  "version": "1.0.0",
  "private": true,
  "description": "云积互动互动中心商家端项目",
  "author": "李武帝",
  "scripts": {
    "commit": "git-cz",
    "serve": "vue-cli-service serve --mode development",
    "dev": "vue-cli-service serve --mode development",
    "serve:debug": "vue-cli-service serve --mode debug",
    "serve:test": "vue-cli-service serve --mode test",
    "build": "vue-cli-service build --mode development",
    "build:test": "vue-cli-service build --mode test",
    "build:analyze": "vue-cli-service build --mode analyze",
    "build:prod": "vue-cli-service build --mode production",
    "build:ali-prod": "vue-cli-service build --mode production --dirname=ali-prod",
    "build:ali-test": "vue-cli-service build --mode production --dirname=ali-test",
    "build:jd-prod": "vue-cli-service build --mode production --dirname=jd-prod",
    "build:jd-test": "vue-cli-service build --mode production --dirname=jd-test",
    "build:tb-prod": "vue-cli-service build --mode production --dirname=tb-prod",
    "build:tb-test": "vue-cli-service build --mode development --dirname=tb-test",
    "deploy": "ciri-upload up ./ciri_config/.ciri-upload.config.js",
    "deploy:debug": "ciri-upload up --config ./ciri_config/.ciri-upload.debug.config.js",
    "deploy:prod": "ciri-upload up --config ./ciri_config/.ciri-upload.prod.config.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "lint-css": "stylelint src/**/*.{css,less,scss,vue} --fix",
    "sort": "npx sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -x @commitlint/config-conventional -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "(src/**/*.{ts,js,vue})|__tests__/**/*.js": [
      "pretty-quick --staged",
      "eslint --fix"
    ],
    "src/**/*.{css,less,scss,vue}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "ant-design-vue": "^2.2.0",
    "axios": "^0.21.1",
    "clone": "^2.1.2",
    "core-js": "^3.6.5",
    "md5": "^2.3.0",
    "nprogress": "^0.2.0",
    "qrcode.vue": "^3.3.3",
    "vue": "^3.2.0",
    "vue-clipboard3": "^2.0.0",
    "vue-color-input": "^1.0.6",
    "vue-router": "^4.0.0-0",
    "vue3-quill": "^0.2.6",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/chai": "^4.2.11",
    "@types/lodash": "^4.14.178",
    "@types/lodash-es": "^4.17.5",
    "@types/mocha": "^5.2.4",
    "@types/nprogress": "^0.2.0",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "babel-plugin-import": "^1.13.3",
    "chai": "^4.1.2",
    "commitizen": "^4.2.4",
    "compression-webpack-plugin": "^9.2.0",
    "cropperjs": "^1.5.12",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "happypack": "^5.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^4.3.8",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.2.7",
    "lodash-es": "^4.17.21",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-resources-loader": "^1.3.2",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^1.4.4",
    "typescript": "4.3.5",
    "uuid": "^8.3.2",
    "vue-cli-plugin-style-resources-loader": "~0.1.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpackbar": "^5.0.2"
  },
  "frameType": "vue3",
  "yjf-config": {
    "frameType": "vue3",
    "startPage": "src/"
  }
}
